web: cd api; PYTHONUNBUFFERED=true gunicorn project.wsgi -b "0.0.0.0:$PORT" --workers=4 --reload
beat: cd api; DJANGO_SETTINGS_MODULE=project.envs.dev C_FORCE_ROOT=true celery beat --app inkshop
celery: cd api; DJANGO_SETTINGS_MODULE=project.envs.dev C_FORCE_ROOT=true celery worker -c 4 -Q celery --app inkshop
