web: PYTHONUNBUFFERED=true gunicorn inkshop.wsgi -b "0.0.0.0:$PORT" --workers=4 --reload
celery: rm celerybeat.pid; DJANGO_SETTINGS_MODULE=inkshop.envs.dev C_FORCE_ROOT=true celery worker -c 4 -Q celery --app inkshop
beat: rm celerybeat.pid; DJANGO_SETTINGS_MODULE=inkshop.envs.dev C_FORCE_ROOT=true celery beat --app inkshop
